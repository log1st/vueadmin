{
  "name": "@vueadmin/admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite preview",
    "build": "bun run build && vite build",
    "check": "vue-tsc",
    "lint": "bun run check && eslint . --fix --ext ts,tsx --report-unused-disable-directives",
    "gen:cti": "cti -b -t src/entities/** src/shared/** src/widgets/** src/features/** src/pages/** src/app/** src/processes/** -e dev -e assets -e pages/**/ui -i *.vue && rm -rf src/pages/**/index.ts"
  },
  "dependencies": {
    "@vueadmin/backend": "workspace:*",
    "vue": "^3.4.20",
    "zod": "^3.22.4",
    "pinia": "^2.1.7",
    "vue-i18n":"^9.9.1",
    "intl-messageformat": "^10.5.11",
    "vue-router": "^4.3.0",
    "@tanstack/vue-query": "^5.24.1",
    "@trpc/client": "next",
    "@trpc/server": "next",
    "superjson": "^2.2.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vue-tsc": "^1.8.27",
    "@vueadmin/eslint-config": "workspace:*",
    "@total-typescript/ts-reset": "^0.5.1",
    "create-ts-index": "^1.14.0",
    "vite-tsconfig-paths": "^4.3.1",
    "sass": "^1.71.1"
  }
}
